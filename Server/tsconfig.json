 {
  "compilerOptions": {
	"rootDir": ".",
	"outDir": "./dist",
    "target": "ES2020",
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "noImplicitAny": false,
	"baseUrl": ".",
    "paths": {
		"@/*": ["./src/*"]
    }
  },
  "types": ["node"],
  "include": ["."],
  "exclude": ["node_modules"]
}
